{
  "name": "npm-dashboard",
  "description": "Dashboard to show download counts for any repositories you want.",
  "version": "1.0.0",
  "author": {
    "name": "Justin Lettau",
    "email": "me@justinlettau.com",
    "url": "https://justinlettau.com"
  },
  "license": "MIT",
  "homepage": "https://justinlettau.github.io/npm-dashboard",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinlettau/npm-dashboard"
  },
  "bugs": {
    "url": "https://github.com/justinlettau/npm-dashboard/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "lint": "eslint . --ext .js,.ts",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.8",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "gh-pages": "^3.2.3",
    "jest": "^27.3.1",
    "postcss": "^8.4.5",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.2",
    "tailwindcss": "^3.0.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  }
}
